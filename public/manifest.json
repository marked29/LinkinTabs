{
  "name": "LinkinTabs",
  "description": "Extension to grab links from tabs and save them to a file.",
  "version": "1.0",
  "manifest_version": 3,

  "action": {
    "default_popup": "index.html",
    "default_icon": "icons/icon48.png"
  },

  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
