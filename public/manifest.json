{
  "name": "LinkinTabs",
  "description": "Extension to grab links from tabs and save them to a file.",
  "version": "1.0",
  "manifest_version": 3,

  "action": {
    "default_popup": "index.html"
  },
   

  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
